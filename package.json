{
  "name": "react-native-calendar-line",
  "description": "Calendar Component for React Native",
  "author": "Felipe Battistotti <felipe.battistotti@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/FelipeBattistotti/react-native-calendar-line#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FelipeBattistotti/react-native-calendar-line.git"
  },
  "version": "1.0.2",
  "main": "index.js",
  "scripts": {
    "eslint": "eslint src/**/*.js __tests__/**/*.js --fix",
    "test": "npm run eslint && jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "postinstall": "node-git-hooks"
  },
  "dependencies": {
    "moment": ">=2.0.0",
    "node-git-hooks": "^1.0.1",
    "prop-types": "^15.6.0",
    "recyclerlistview": "^3.0.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-native": "^0.63.37",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^26.6.3",
    "babel-preset-react-native": "^4.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.3.0",
    "eslint": "^4.13.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^26.6.3",
    "react": "^16.13.1",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.2.0",
    "react-native": "^0.62.0",
    "react-test-renderer": "^16.2.0"
  },
  "keywords": [
    "RN",
    "calendar",
    "calendar line",
    "calendar-line",
    "native",
    "react",
    "react native",
    "react native calendar",
    "react native calendar line",
    "react-native",
    "react-native-calendar",
    "react-native-calendar-line",
    "rn",
    "line"
  ],
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "verbose": true,
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/img.js",
      "!example/*.js"
    ],
    "modulePathIgnorePatterns": [
      "example",
      "npm-cache",
      ".npm"
    ],
    "collectCoverage": false,
    "globals": {
      "__DEV__": true
    }
  }
}